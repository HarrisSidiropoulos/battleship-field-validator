{
  "name": "battleship-field-validator",
  "version": "1.0.0",
  "description": "Battleship field validator",
  "main": "dist/index.js",
  "scripts": {
    "check-coverage": "npm t -- --coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start": "npm run watch:test",
    "watch:test": "npm t -- --watch",
    "test": "jest",
    "lint": "node_modules/.bin/eslint src",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Battleship",
    "field",
    "validator",
    "codewars"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Harris Sidiropoulos",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-eslint": "7.2.3",
    "babel-jest": "^17.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.7.2",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.1.0",
    "ghooks": "1.2.1",
    "jest": "^17.0.3",
    "mocha": "2.4.5",
    "npm-run-all": "1.7.0",
    "nyc": "6.4.0",
    "rimraf": "2.5.2",
    "webpack": "1.13.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
